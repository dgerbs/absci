					<footer>
						<a href="#product" class="button button-big">Products</a>
					</footer>
				</article>
			</div>

		<!-- Product -->
			<div class="wrapper wrapper-style3">
				<article id="product">
					<header>
						<h2>Bridging gaps.</h2>
						<span>Engineering efficiency.</span>
					</header>
